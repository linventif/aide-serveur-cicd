	},
	"homepage": "https://github.com/linventif/aide-serveur-cicd#readme",
	"license": "MIT",
	"dependencies": {
		"dotenv": "^17.2.0",
		"puppeteer": "^24.12.1"
	},
	"devDependencies": {
		"typescript": "^5.0.0",
		"@types/node": "^20.0.0"
	}
}
